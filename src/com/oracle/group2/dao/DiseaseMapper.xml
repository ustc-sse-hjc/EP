<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://www.mybatis.org/dtd/mybatis-3.dtd">

<mapper namespace="com.oracle.group3.dao.DiseaseMapper">
	<select id="getDiseaseByName" resultType="Disease">
		select * from Disease
		where Disease_name=#{0}
	</select>
	
	<select id="getDiseaseById" resultType="Disease">
		select * from Disease
		where Disease_id=#{0}
	</select>
</mapper>